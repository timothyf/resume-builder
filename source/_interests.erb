<% if @resume_data.interests? %>
    <div class="interests-container container-block">
        <h2 class="container-block-title"><%= @resume_data.headers.interests %></h2>
        <ul class="list-unstyled interests-list">
            <% @resume_data.interests.each do |interest| %>
                <li><%= interest %></li>
            <% end %>
        </ul>
    </div>
<% end %>
