<% partial(:data, :locals => { :resume_data => nil }) %>

<head>
  <%= partial 'header' %>
  <link id="theme-style" rel="stylesheet" href="./stylesheets/pdf.css">
</head>

<body>
    <div class="wrapper">
      <div class="sidebar-wrapper">
        <% @layout_data.content.right.each do |section| %>
          <%= partial section %>
        <% end %>
      </div>

      <div class="main-wrapper">
        <% @layout_data.content.center.each do |section| %>
          <%= partial section, locals: {brief: true} %>
        <% end %>
      </div>
    </div>
    <%= partial 'footer' %>
</body>

</html>
